<div>
	<form method="post" action="<%= baseUrl %>category">
		<div>
			<label>Category</label>
			<input type="text" required="" name="category">
		</div>
		<div>
			<label>Status</label>
			<select name="status">
				<option value="">---</option>
				<option value="1">Active</option>
				<option value="0">De-Active</option>
			</select>
		</div>
		<div>
			<button type="submit" style="background-color: blue">Submit</button>
		</div>
	</form>
</div>
<br>
<div>
	<table>
		<thead>
			<tr>
				<th>S.No</th>
				<th>Category</th>
				<th>Status</th>
				<th>Date & Time</th>
			</tr>
		</thead>
		<tbody>
			<% if(category && category.length){ category.find((cat,i) => { %>
			<tr>
				<td><%= i+1 %></td>
				<td><%= cat.category %></td>
				<td><%= cat.status %></td>
				<td><%= cat.credate %></td>
			</tr>
			<% }) } else { %>
				<tr>
					<td colspan="4">No data found.</td>
				</tr>
			<% } %>
		</tbody>
	</table>
</div>
<script type="module" src="assets/js/index.js"></script>